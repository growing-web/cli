{
  "schemaVersion": "0.0.0",
  "publicPath": "/",
  "entries":{
      "index": "src/index.js"
  },
  "exports": {
      "index": {
          "esm": "index.js",
          "system": "index.system.js"
      }
  },
 "formats": {
      "index": ["esm", "system"]
  },
  "importmap": {
      "packageName": "${packageName}",
      "filename": {
          "esm": "importmap.json",
          "system": "system-importmap.json"
      }
  },
  "outDir": "dist"
}
