{
  "schemaVersion": "0.0.0",
  "publicPath": "/",
  "entries": {
    "index": "src/main.ts"
  },
  "exports": {
    "index": {
      "esm": "index.js",
      "system": "index.system.js"
    }
  },
  "formats": {
    "index": ["esm", "system"]
  },
  "importmap": {
