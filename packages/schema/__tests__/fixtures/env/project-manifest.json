{
  "schemaVersion": "1.0.0",
  "entry": "src/main.ts",
  "publicPath": "/",
  "outDir": "dist",
  "formats": ["cjs", "esm"],
  "externals": {
    "vue": "Vue"
  },
  "server": {
    "port": 3000,
    "host": "http://dev.test.com",
    "proxy": [
      {
        "url": "/api",
        "target": "http://localhost:8080/api?a=1&b=1",
        "pathRewrite": [
          {
            "regular": "^/api",
            "replacement": ""
          }
        ]
      }
    ]
  },
  "env": {
    "development": {
      "publicPath": "/"
    },
    "production": {
      "publicPath": "http://dev.test.com",
      "externals": {
        "react": "React"
      },
      "server": {
        "port": 3200
      }
    }
  }
}
