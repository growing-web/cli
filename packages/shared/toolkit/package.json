{
  "name": "@growing-web/web-builder-toolkit",
  "description": "@growing-web/web-builder-toolkit for Growing Web Guidelines.",
  "version": "0.0.1",
  "keywords": [
    "growing-web",
    "web-builder",
    "utils"
  ],
  "files": [
    "dist"
  ],
  "license": "UNLICENSED",
  "author": "vben",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm unbuild",
    "dev": "pnpm unbuild --stub",
    "dts-gen": "pnpm esno ./scripts/dts-gen",
    "prepublishOnly": "pnpm run build",
    "clean": "pnpm rimraf .turbo && pnpm rimraf node_modules && pnpm rimraf dist",
    "prepack": "pnpm unbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/web-builder.git",
    "directory": "packages/shared/toolkit"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/growing-web/web-builder/issues"
  },
  "homepage": "https://github.com/growing-web/web-builder#readme",
  "dependencies": {
    "@growing-web/web-builder-constants": "workspace:*",
    "consola": "^2.15.3",
    "latest-version": "^6.0.0",
    "pathe": "^0.2.0",
    "picocolors": "^1.0.0",
    "pkg-types": "^0.3.2",
    "semver": "^7.3.5",
    "strip-json-comments": "^4.0.0",
    "unctx": "^1.0.2",
    "vite": "^2.8.2"
  },
  "devDependencies": {
    "@growing-web/web-builder-types": "workspace:*"
  }
}
